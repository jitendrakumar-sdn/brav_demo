<script src="../js/jquery.min.js"></script>
<script src="../js/bootstrap.min.js"></script>
<script>
    $(document).ready(function() {
        $("#chat").click(function() {
            $(".chat-panel").toggle();
        });
        $("#notes").click(function() {
            $(".notes-panel").toggle();
        });
        $("#alert-msg").click(function() {
            $(".alert-panel").toggle();
        });

    });

</script>
